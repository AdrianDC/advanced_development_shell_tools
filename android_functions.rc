  local target=$(git remote -v \
               | head -n 1 \
               | awk '{ print $2 }' \
               | sed "s/\(http:\/\/\|https:\/\/\|git:\/\/\)\(.*\)\/[^\/]*\/\(.*\)/http:\/\/\2\/$project\/\3/");
