# === Android builds ===
function makesep()
{
  cwd=$(pwd);
  repor;
  export USE_NINJA=false;
  if [ -d ./system/sepolicy ]; then
    mmm -B -j$(grep -c ^processor /proc/cpuinfo) ./system/sepolicy/ | tee $LogFile;
  else
