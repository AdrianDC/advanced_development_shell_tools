function devuplboot()
{
  local cwd=$(pwd);
  repor;

  device="$1";
  mv "./out/target/product/$device/boot.img" "./out/target/product/$device/boot-$device.img";
  devupl "./out/target/product/$device/boot-$device.img";

  cd "$cwd";
