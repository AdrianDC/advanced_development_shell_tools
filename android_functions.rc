# === Repo Branch ===
function repogetbranch()
{
  local branch='';
  local infos=$(repo info . 2>&1);
  if [[ ! "$infos" =~ 'repo to be installed' ]] && [[ ! "$infos" =~ 'sudo apt' ]]; then
    branch=$(echo "$infos" \
          | grep -i 'Manifest branch' \
          | head -n 1 \
          | sed 's/.*\/heads\/\(.*\)/\1/');
  fi;
  if [ -z "$branch" ] || [[ "$branch" =~ "Manifest" ]]; then
    branch="cm-13.0";
  fi;
  echo "$branch";
}

