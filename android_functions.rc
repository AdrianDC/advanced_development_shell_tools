function devuplrom()
{
  local cwd=$(pwd);
  repor;

  device="$1";
  target="$2";
  if [ -z "$target" ]; then
    target="Development";
  fi;
  romfiles=$(find "./out/target/product/$device/"*"$device"*".zip" -mtime -1 | sort);
  romfile="";
  for romfiletest in $romfiles; do
    if [ -f "$romfiletest.md5sum" ]; then
      romfile="$romfiletest";
    fi;
  done;
  if [ -z "$romfile" ]; then
    romfile=$(echo "$romfiles" | awk '{ print $NF }');
  fi;
  if [ ! -z "$romfile" ]; then
     fileupl "$romfile" "$target";
  fi;

  cd "$cwd";
}

