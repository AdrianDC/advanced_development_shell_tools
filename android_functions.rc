# === Zip AutoPackAndSign ===
function autozipsign()
{
  name=$(echo "$1" | sed 's/\///g');
  rm $name.zip;
  cd $name/;
  rm -f META-INF/CERT.RSA;
  rm -f META-INF/CERT.SF;
  rm -f META-INF/MANIFEST.MF;
  rm -rf META-INF/com/android;
  zip ../$name.zip * -r;
  cd ..;
  if [ -z "$2" ]; then
    zipsign $name.zip;
  fi;
  adb sideload $name.zip
}

